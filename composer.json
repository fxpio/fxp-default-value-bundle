{
    "name": "sonatra/default-value-bundle",
    "type": "symfony-bundle",
    "description": "Creates or injects the default values ​​with a advanced configuration in the object",
    "keywords": ["default value", "object", "entity", "document"],
    "homepage": "https://github.com/sonatra/sonatra-default-value-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "François Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.0",
        "symfony/framework-bundle": "^3.0.0",
        "sonatra/default-value": "^1.0.0"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\DefaultValueBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
